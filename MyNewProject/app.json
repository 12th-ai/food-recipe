{
    "expo": {
        "name": "Recipe App Generator",
        "slug": "RecipeAppGenerator",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/large.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/large.png",
            "resizeMode": "contain",
            "backgroundColor": "#fdb15a"
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.mihigojordan.MyNewProject",
            "buildNumber": "1.0.0"
        },
        "android": {
            "package": "com.mihigojordan.MyNewProject",
            "versionCode": 1,
            "permissions": [
                "CAMERA",
                "READ_EXTERNAL_STORAGE",
                "WRITE_EXTERNAL_STORAGE",
                "INTERNET"

            ],
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/large.png",

                "backgroundColor": "#fdb15a"
            }
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router"
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "eas": {
                "projectId": "85a284b0-eb35-42a5-a4ed-5f5a37659808"
            }
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "runtimeVersion": {
            "policy": "sdkVersion"
        }
    }
}